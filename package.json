{
  "name": "sula-ai",
  "version": "0.1.0",
  "description": "FAB + chat widget for SULA (Sulung Lab Assistant). Use in any web with your own backend and app_id.",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.tsx --format esm --dts --external react --external react-dom",
    "dev": "tsup src/index.tsx --format esm --dts --watch --external react --external react-dom"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "sula",
    "chat",
    "assistant",
    "fab",
    "widget"
  ],
  "author": "jaja valentino",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jaja2302/sula-ai-packet-npm.git",
    "directory": "sula-ai"
  }
}